{
  "name": "solid-suggest",
  "version": "1.0.0",
  "description": "Headless search suggestion dropdown UI library for SolidJS",
  "homepage": "https://antrikshy.com/solid-suggest",
  "repository": {
    "type": "git",
    "url": "https://github.com/Antrikshy/solid-suggest.git"
  },
  "bugs": {
    "url": "https://github.com/Antrikshy/solid-suggest/issues"
  },
  "keywords": [
    "solidjs",
    "search",
    "suggestions",
    "dropdown",
    "autocomplete",
    "ui"
  ],
  "author": "Antriksh Yadav",
  "license": "ISC",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "devOnly": "rollup -c -w",
    "devDemo": "npx http-server -p 8000",
    "devWithDemo": "concurrently \"npm:devOnly\" \"npm:devDemo\"",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "solid-js": "^1.9.x"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "babel-preset-solid": "^1.9.6",
    "concurrently": "^9.2.0",
    "rollup": "^4.42.0",
    "solid-js": "^1.9.7",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
